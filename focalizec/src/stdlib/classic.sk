#NAME classic.
nnpp : A : logic.Prop ->
       logic.prf (logic.imply (logic.not (logic.not A))
                              A).

excluded_middle : A : logic.Prop ->
                  logic.prf (logic.or A
                               (logic.not A))
                :=
                  A : logic.Prop =>
                  nnpp (logic.or A (logic.not A))
                       (logic.NNem A).